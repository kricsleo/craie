{
  "name": "craie",
  "type": "module",
  "version": "0.0.11",
  "packageManager": "pnpm@7.12.0",
  "description": "Output colorful logs, much like chalk but works for the browser.",
  "author": "Kricsleo <kricsleo@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/kricsleo/craie#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kricsleo/craie.git"
  },
  "bugs": "https://github.com/kricsleo/craie/issues",
  "keywords": ["console", "log", "color", "browser" , "chalk"],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.global.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=develop rollup -c",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "pnpm": "^7.12.0",
    "typescript": "^4.8.3",
    "vitest": "^0.23.4",
    "@rollup/plugin-commonjs": "^22.0.1",
    "rollup": "^2.75.7",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-typescript2": "^0.32.1"
  }
}
